class student{
    constructor(name,age){
        this.name=name;
        this.age=age;
    }
    display(){
        console.log(this.name);
        console.log(this.age);
        document.write(this.name+ "<br>"+this.age+"<br>");
    }

}
const s1= new student("ram",22);
const s2= new student("pari",33);
s1.display();
s2.display();
